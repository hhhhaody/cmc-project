        }
    },
    "gauge": {
        "axisLine": {
            "show": false
        },
        "splitLine": {
            "show": false
        },
        "axisTick": {
            "show": false
        },
        "axisLabel": {
            "show": false
        },
        "pointer": {
            "show": true,
            "icon": "triangle",
            "offsetCenter": [
                "0%",
                "-55%"
            ],
            "length": "6%",
            "width": "2em",
            "itemStyle": {
                "color": "#fff",
                "opacity": 0.8,
                "shadowColor": "#2495fd",
                "shadowBlur": 20
            }
        },
        "detail": {
            "color": "#fff",
            "fontSize": "0.1vh",
            "textShadowColor": "#a3ccf9",
            "textShadowBlur": 5,
            "textShadowOffsetX": -1,
            "textShadowOffsetY": 1,
            "valueAnimation": true,
            "offsetCenter": [
                0,
                "0%"
            ],
            "formatter": "{value}%"
        },
        "itemStyle": {
            "color": {
                "type": "linear",
                "x": 0,
                "y": 0,
                "x2": 0,
                "y2": 1,
                "colorStops": [
                    {
                        "offset": 1,
